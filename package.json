{
  "name": "wunder-drupal-webpack",
  "description": "A simple starter theme for Drupal 8 & 9 projects based on Classy",
  "version": "1.0.0",
  "author": "Wunder",
  "license": "ISC",
  "scripts": {
    "start": "npm-run-all -p watch sync",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "sync": "node browser-sync.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ],
  "proxy": "http://wunder-drupal-webpack.lndo.site/",
  "paths": {
    "appThemeRoot": "web/themes/custom/wunder-theme"
  },
  "dependencies": {
    "object-fit-images": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.13",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "file-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^3.3.12"
  }
}
